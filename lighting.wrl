#VRML V2.0 utf8

Group{
children [
DirectionalLight{
direction 0 -1 0  # light from below
intensity 0.5
}

DirectionalLight{
direction 1 0 0  # softer light from left
intensity 0.25
}

Shape {
appearance Appearance {
material Material {
diffuseColor 0 0 1
}
}
geometry Sphere { }
}  # end shape

]  # end children

}  # end group
